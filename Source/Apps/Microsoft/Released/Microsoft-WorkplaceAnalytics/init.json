        "displayname": "Waiting for API Application Settings Deployment",
        "name": "Microsoft-DeployAzureWebAppApplicationSettings",
        "DeploymentName": "ApiApplicationSettingsDeployment",
        "SettingName": "ApiAppSettings",
        "webAppName": "$run( this.MS.DataStore.getValue('siteName') + '-api' )"
      },
      {
        "DeploymentName": "WebAppDeployment",
        "RepoUrl": "https://github.com/v-preben/Demo.git",
        "displayname": "Waiting for Keyline Web UI",
        "name": "Microsoft-DeployAzureWebApp",
        "webAppName": "$run( this.MS.DataStore.getValue('siteName') )",
        "Project": "OnaWebApp/OnaWebApp.csproj",
        "ArmTemplateName": "AzureWebApp",
        "branch": "master"
      },
      {
        "displayname": "Waiting for Keyline Web UI",
        "name": "Microsoft-WaitForWpaArmDeploymentStatus",
        "DeploymentName": "WebAppDeployment"
      },
      {
        "WebAppSettings": {
          "hostName": "$run( this.MS.DataStore.getValue('siteName') + '.azurewebsites.net' )",
          "wpaDataAdapterHostName": "$run( this.MS.DataStore.getValue('siteName') + '.azurewebsites.net' )",
          "SPNKey": "$run( this.MS.DataStore.getValue('SPNKey') )",
          "SPNAppId": "$run( this.MS.DataStore.getValue('SPNAppId') )",
          "APUrl": "$run( this.MS.DataStore.getValue('SPNAppId') )",
          "TenantId": "$run( this.MS.DataStore.getValue('SPNTenantId') )"
        },
        "displayname": "Waiting for Web App Application Settings Deployment",
        "name": "Microsoft-DeployAzureWebAppApplicationSettings",
        "DeploymentName": "WebAppApplicationsSettingsDeployment",
        "SettingName": "WebAppSettings"
      }
